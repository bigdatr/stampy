{
  "name": "stampy",
  "version": "0.0.0",
  "description": "The new and improved stampy",
  "repository": {
    "type": "git",
    "url": "git@github.com:bigdatr/stampy.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/babel src -d lib --watch",
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "ava": "ava",
    "test": "nyc --report-dir=./coverage ava && npm run check-coverage",
    "check-coverage": "nyc check-coverage --branches 0.0  --functions 0.0 --lines 0",
    "docs": "jsdoc -c jsdoc/conf.json",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "devDependencies": {
    "app-module-path": "^2.0.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-blueflag": "^0.0.1",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.5.0",
    "enzyme": "^2.5.1",
    "eslint": "^2.7.0",
    "eslint-config-blueflag": "^0.2.0",
    "flow-bin": "^0.33.0",
    "git-url-parse": "^6.0.1",
    "jsdoc": "^3.4.2",
    "jsdoc-babel": "^0.2.1",
    "jsdonk": "0.1.4",
    "nyc": "^6.6.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.3.2"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "immutable-recursive": "^0.0.6"
  },
  "main": "lib/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit",
    "files": [
      "src/**/*-test.js"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/*-test.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "babel-register"
    ]
  }
}
