{
  "name": "stampy",
  "version": "0.0.0",
  "description": "The new and improved stampy",
  "repository": {
    "type": "git",
    "url": "git@github.com:bigdatr/stampy.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/babel src -d lib --watch",
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "ava": "ava",
    "test": "nyc --report-dir=./coverage npm run ava && npm run check-coverage",
    "check-coverage": "nyc check-coverage --branches 0.0  --functions 0.0 --lines 0",
    "docs": "jsdoc -c jsdoc/conf.json",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "devDependencies": {
    "app-module-path": "^2.0.0",
    "ava": "^0.16.0",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-config-blueflag": "^0.1.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "flow-bin": "^0.33.0",
    "git-url-parse": "^6.0.1",
    "jsdoc": "^3.4.2",
    "jsdoc-babel": "^0.2.1",
    "jsdonk": "0.1.3",
    "nyc": "^6.6.1",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.0.0",
    "react-docgen": "^2.10.0",
    "react-dom": "^15.3.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0"
  },
  "main": "lib/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "files": [
      "src/**/*-test.js"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/*-test.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "require": [
      "./tests/tests.js"
    ]
  }
}
